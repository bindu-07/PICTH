<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_forgot_password"
    app:startDestination="@+id/forgotPasswordFragment1">
    <fragment
        android:id="@+id/forgotPasswordFragment1"
        android:name="com.photo.picth.ui.fragments.forgotPassword.ForgotPasswordFragmentOne"
        android:label="fragment_forgot_password1">
        <action
            android:id="@+id/action_forgotPasswordFragment1_to_forgotPasswordFragment2"
            app:destination="@+id/forgotPasswordFragment2"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordFragment2"
        android:name="com.photo.picth.ui.fragments.forgotPassword.ForgotPasswordFragmentTwo"
        android:label="fragment_forgot_password2">
        <action
            android:id="@+id/action_forgotPasswordFragment2_to_forgotPasswordFragment3"
            app:destination="@+id/forgotPasswordFragment3"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordFragment3"
        android:name="com.photo.picth.ui.fragments.forgotPassword.ForgotPasswordFragmentThree"
        android:label="fragment_forgot_password3" />
</navigation>
